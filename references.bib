@article{Wilson2006a,
  title = {Learning Phonology with Substantive Bias: An Experimental and Computational Study of Velar Palatalization.},
  author = {Wilson, Colin},
  year = {2006},
  month = sep,
  journal = {Cognitive science},
  volume = {30},
  number = {5},
