@article{nishiharaGeneralAnalysisConvergence2015,
  title = {A {{General Analysis}} of the {{Convergence}} of {{ADMM}}},
  author = {Nishihara, Robert and Lessard, Laurent and Recht, Benjamin and Packard, Andrew and Jordan, Michael I.},
  year = {2015},
