  isbn = {1603.00788},
  keywords = {approximate inference,Bayesian inference,probabilistic programming},
  file = {/Users/wdt/Sync/work/library/zotero-files/storage/HKFLNFPZ/m-api-1cf8497c-3312-e113-74f2-d811a695e54b.pdf}
}

