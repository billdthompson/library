@article{Faruqui2014,
  title = {Retrofitting {{Word Vectors}} to {{Semantic Lexicons}}},
  author = {Faruqui, Manaal and Dodge, Jesse and Jauhar, Sujay K. and Dyer, Chris and Hovy, Eduard and Smith, Noah A.},
  year = {2014},
