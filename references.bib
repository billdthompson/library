@article{Miu2018a,
  title = {Innovation and Cumulative Culture through Tweaks and Leaps in Online Programming Contests},
  author = {Miu, Elena and Gulley, Ned and Laland, Kevin N. and Rendell, Luke},
  year = {2018},
  month = dec,
  journal = {Nature Communications},
