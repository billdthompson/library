@article{Nam2013b,
  title = {Computational Simulation of {{CV}} Combination Preferences in Babbling.},
  author = {Nam, Hosung and Goldstein, Louis M and Giulivi, Sara and Levitt, Andrea G and Whalen, D H},
  year = {2013},
