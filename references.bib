@article{Mitra2018a,
  title = {Fast {{Convergence}} for {{Stochastic}} and {{Distributed Gradient Descent}} in the {{Interpolation Limit}}},
  author = {Mitra, Partha P},
  year = {2018},
  month = mar,
  eprint = {1803.02922},
  urldate = {2018-04-08},
  abstract = {Modern supervised learning techniques, particularly those using so called deep nets, involve fitting high dimensional labelled data sets with functions containing very large numbers of parameters. Much of this work is empirical, and interesting phenomena have been observed that require theoretical explanations, however the non-convexity of the loss functions complicates the analysis. Recently it has been proposed that some of the success of these techniques resides in the effectiveness of the simple stochastic gradient descent algorithm in the so called interpolation limit in which all labels are fit perfectly. This analysis is made possible since the SGD algorithm reduces to a stochastic linear system near the interpolating minimum of the loss function. Here we exploit this insight by analyzing a distributed algorithm for gradient descent, also in the interpolating limit. The algorithm corresponds to gradient descent applied to a simple penalized distributed loss function, \$L(\{\textbackslash bf w\}\_1,...,\{\textbackslash bf w\}\_n) = \textbackslash Sigma\_i l\_i(\{\textbackslash bf w\}\_i) + \textbackslash mu \textbackslash sum\_\{{$<$}i,j{$>\rbrace$}|\{\textbackslash bf w\}\_i-\{\textbackslash bf w\}\_j|\^2\$. Here each node is allowed its own parameter vector and \${$<$}i,j{$>\$$} denotes edges of a connected graph defining the communication links between nodes. It is shown that this distributed algorithm converges linearly (ie the error reduces exponentially with iteration number), with a rate \$1-\textbackslash frac\{\textbackslash eta\}\{n\}\textbackslash lambda\_\{min\}(H)\vphantom\}},
  archiveprefix = {arxiv},
  file = {/Users/wdt/Sync/work/library/zotero-files/storage/3II3GIMR/2018 - Mitra - Fast Convergence for Stochastic and Distributed Gradient Descent in the Interpolation Limit.pdf;/Users/wdt/Sync/work/library/zotero-files/storage/E3F4PFVY/2018 - Mitra - Fast Convergence for Stochastic and Distributed Gradient Descent in the Interpolation Limit(2).pdf}
