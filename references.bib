@article{Miton2018a,
  title = {Cumulative Culture in the Laboratory: Methodological and Theoretical Challenges},
  author = {Miton, Helena and Charbonneau, Mathieu},
  year = {2018},
  month = may,
  journal = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {285},
