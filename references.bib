}

@article{Westbury2016a,
  title = {Telling the World's Least Funny Jokes: {{On}} the Quantification of Humor as Entropy},
  author = {Westbury, Chris and Shaoul, Cyrus and Moroschan, Gail and Ramscar, Michael},
  year = {2016},
