@article{Waterman2017b,
  title = {Sensorimotor Control Dynamics and Cultural Biases: Learning to Move in the Right (or Left) Direction},
  author = {Waterman, A.H. Amanda H. and Giles, Oscar T. O.T. and Havelka, Jelena and Ali, Sumaya and Culmer, P.R. Peter R. and Wilkie, R.M. Richard M. and {Mon-Williams}, Mark},
  year = {2017},
