@article{Sexton2016,
  title = {Learning an {{Optimization Algorithm}} through {{Human Design Iterations}}},
  author = {Sexton, Thurston and Ren, Max Yi},
  year = {2016},
  month = aug,
  journal = {Journal of Mechanical Design},
  volume = {139},
