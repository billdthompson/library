@article{crandallCooperatingMachines2018,
  title = {Cooperating with Machines},
  author = {Crandall, Jacob W. and Oudah, Mayada and {Tennom} and {Ishowo-Oloko}, Fatimah and Abdallah, Sherief and Bonnefon, Jean Fran{\c c}ois and Cebrian, Manuel and Shariff, Azim and Goodrich, Michael A. and Rahwan, Iyad},
  year = {2018},
