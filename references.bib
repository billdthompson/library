@article{Gebuis2012,
  title = {The Role of Visual Information in Numerosity Estimation.},
  author = {Gebuis, Titia and Reynvoet, Bert},
  year = {2012},
  month = jan,
  journal = {PloS one},
  volume = {7},
  number = {5},
