@article{DeDomenico2017a,
  title = {Modeling Structure and Resilience of the Dark Network},
  author = {De Domenico, Manlio and Arenas, Alex},
  year = {2017},
  month = feb,
  journal = {Physical Review E},
  volume = {95},
