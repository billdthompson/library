  title = {The {{Bayesian Echo Chamber}}: {{Modeling Social Influence}} via {{Linguistic Accommodation}}},
  author = {Guo, Fangjian and Blundell, Charles and Wallach, Hanna and Heller, Katherine},
  year = {2014},
