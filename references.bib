@article{Riedl2018,
  title = {Product Diffusion through On-Demand Information-Seeking Behaviour},
  author = {Riedl, Christoph and Bjelland, Johannes and Canright, Geoffrey and Iqbal, Asif and {Eng{\o}-Monsen}, Kenth and Qureshi, Taimur and Sunds{\o}y, P{\aa}l Roe and Lazer, David},
  year = {2018},
