@article{Kroger2009,
  title = {Towards a Neurocomputational Model of Speech Production and Perception},
  author = {Kr{\"o}ger, Bernd J. and Kannampuzha, Jim and {Neuschaefer-Rube}, Christiane},
  year = {2009},
