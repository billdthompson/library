@article{Miguez2007,
  title = {Analysis of Selection Methods for Cost-Reference Particle Filtering with Applications to Maneuvering Target Tracking and Dynamic Optimization},
  author = {M{\'i}guez, Joaqu{\'i}n},
  year = {2007},
