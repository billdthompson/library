@article{Vallabha2007,
  title = {Unsupervised Learning of Vowel Categories from Infant-Directed Speech.},
  author = {Vallabha, Gautam K and McClelland, James L and Pons, Ferran and Werker, Janet F and Amano, Shigeaki},
  year = {2007},
