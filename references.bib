@article{Wolpert1997,
  title = {No Free Lunch Theorems for Optimization},
  author = {Wolpert, David H. and Macready, William G.},
  year = {1997},
  month = apr,
  journal = {IEEE Transactions on Evolutionary Computation},
  volume = {1},
  number = {1},
  pages = {67--82},
