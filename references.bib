@article{Acerbi2011,
  title = {Modeling Imitation and Emulation in Constrained Search Spaces},
  author = {Acerbi, Alberto and Tennie, Claudio and Nunn, Charles L.},
  year = {2011},
  month = jun,
  journal = {Learning \& Behavior},
