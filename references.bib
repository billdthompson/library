@article{Shafto2012,
  title = {Epistemic Trust: Modeling Children's Reasoning about Others' Knowledge and Intent.},
  author = {Shafto, Patrick and Eaves, Baxter and Navarro, Daniel J and Perfors, Amy},
  year = {2012},
  month = may,
  journal = {Developmental science},
  volume = {15},
  number = {3},
