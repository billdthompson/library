@article{Zerjal2001,
  title = {Geographical, Linguistic, and Cultural Influences on Genetic Diversity: {{Y-chromosomal}} Distribution in {{Northern European}} Populations.},
  author = {Zerjal, T and Beckman, L and Beckman, G and Mikelsaar, A V and Krumina, A and Kucinskas, V and Hurles, M E and {Tyler-Smith}, C},
  year = {2001},
