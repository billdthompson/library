@article{Draghi2010,
  title = {Mutational Robustness Can Facilitate Adaptation},
  author = {Draghi, Jeremy A. and Parsons, Todd L. and Wagner, G{\"u}nter P. and Plotkin, Joshua B.},
  year = {2010},
  month = jan,
  journal = {Nature},
  volume = {463},
