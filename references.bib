@article{alves_case_2020,
  title = {A Case Report: Insights into Reducing Plastic Waste in a Microbiology Laboratory},
  shorttitle = {A Case Report},
  author = {Alves, Joana and Sargison, Fiona A and Stawarz, Hanne and Fox, Willow B and Huete, Samuel G and Hassan, Amany and McTeir, Brian and Pickering, Amy C},
